{
	"title": "Java Foundation",
	"language": "zh-hans",
	"plugins": [
		"splitter",
		"tbfed-pagefooter",
		"toggle-chapters",
    "copy-code-button",
    "expandable-chapters-small",
    "simple-page-toc",
    "anchors"
	],
	"pluginsConfig": {
		"sharing": {
			"weibo": true,
			"facebook": true,
			"twitter": true,
			"google": false,
			"instapaper": false,
			"vk": false,
			"all": [
				"facebook", "google", "twitter",
				"weibo", "instapaper"
			]
		},
    "simple-page-toc": {
        "maxDepth": 5,
        "skipFirstH1": true
    },
		"tbfed-pagefooter": {
			"copyright": "Copyright &copy baoxiao fang.com 2018",
			"modify_label": "该文件修订时间：",
			"modify_format": "YYYY-MM-DD HH:mm:ss"
		}
	}
}
